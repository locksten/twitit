{
  "name": "@twitit/server",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "private": true,
  "main": "./dist/server.js",
  "scripts": {
    "watch": "tsc-watch --onSuccess \"npm run start\"",
    "start": "NODE_PATH=dist node ./dist/server.js",
    "build": "tsc",
    "gen:db-types": "npx zapatos"
  },
  "eslintConfig": {
    "extends": ".custom-eslintrc.json",
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "@typescript-eslint/no-unused-vars": "off"
        }
      }
    ]
  },
  "dependencies": {
    "@types/pg": "^7.14.7",
    "apollo-server-express": "^2.19.0",
    "express": "^4.17.1",
    "gqtx": "^0.7.0",
    "graphql": "^15.4.0",
    "jest": "^26.4.2",
    "pg": "^8.5.1",
    "zapatos": "^3.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.3",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.20.0",
    "prettier": "^2.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.0.2"
  }
}
